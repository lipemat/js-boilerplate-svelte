.wrap {
	background: #d3d3d3;
	border: 1px solid #000;
	border-radius: 5px;
	display: block;
	margin: 20px auto;
	padding: 25px;
	width: 50%;

	code {
		background: #ff0;
		border: 1px solid #000;
		border-radius: 3px;
		font-weight: 700;
		padding: 5px 8px;
	}
}

:global(.dome) {
	background: #9acd32;
}
